    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-routing"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-traits"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-http"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-session"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-cache"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-database"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-model"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-storage"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-event"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-application"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-component"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-template"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-container"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/phi-configuration"},
    {"type": "vcs", "url": "https://github.com/ElBiniou/dashboard"}
  ],
  "require": {
    "phi-core": "dev-master",
    "phi-traits": "dev-master",
    "phi-routing": "dev-master",
    "phi-http": "dev-master",
    "phi-session": "dev-master",
    "phi-cache": "dev-master",
    "phi-database": "dev-master",
    "phi-model": "dev-master",
    "phi-storage": "dev-master",
    "phi-event": "dev-master",
    "phi-application": "dev-master",
    "phi-component": "dev-master",
    "phi-template": "dev-master",
    "phi-container": "dev-master",
    "phi-configuration": "dev-master",

    "elbiniou-dashboard": "dev-master"
  },
  "require-dev": {

  },
  "config": {
    "process-timeout": 600
  },

  "autoload": {
    "files": [
      "source/helper.php",
      "static_vendor/guzzlehttp/guzzle/src/functions_include.php",
      "static_vendor/guzzlehttp/promises/src/functions_include.php",
      "static_vendor/guzzlehttp/psr7/src/functions_include.php",

      "static_vendor/php-graph-sdk-5.x/src/Facebook/polyfills.php"
    ],
    "psr-4": {
      "ImGrowth\\": "source/class",
      "ImGrowth\\Configuration\\": "Configuration",

      "League\\OAuth1\\": "static_vendor/League/OAuth1/src",

      "Psr\\Http\\Message\\": "static_vendor/psr/http-message/src/",

      "GuzzleHttp\\": "static_vendor/guzzlehttp/guzzle/src/",
      "GuzzleHttp\\Promise\\": "static_vendor/guzzlehttp/promises/src/",
      "GuzzleHttp\\Psr7\\": "static_vendor/guzzlehttp/psr7/src/",

      "Facebook\\": "static_vendor/php-graph-sdk-5.x/src/Facebook/",


      "Cocur\\Slugify\\": "static_vendor/slugify/src"


    }
  }
}